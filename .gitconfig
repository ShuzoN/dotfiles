[user]
	name = ShuzoN
	email = d.namu.r21@gmail.com
[core]
	excludesfile = ~/dotfiles/.gitignore_global
	quotepath = false
	editor = /usr/local/bin/vim
[url "https://"]
	insteadOf=git://
[push]
	default = matching
[alias]
	co  = checkout
  st  = status
  cm  = commit
  lg  = log --graph --decorate --all --oneline
  lgp = log -p
  df  = diff
[fetch]
  prune = true
[grep]
  lineNumber = true
[color "grep"]
  filename = green
  linenumber = yellow
  separator = white
  match = yellow reverse
  trustExitCode = true
